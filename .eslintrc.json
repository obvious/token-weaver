{
  "extends": "./node_modules/gts/",
  "ignorePatterns": "dist",
  "rules": {
    "@typescript-eslint/no-explicit-any": ["off"],
    "node/no-unpublished-import": ["error", {
      "allowModules": ["style-dictionary", "camel-case"]
    }],
    "node/no-extraneous-import": ["error", {
      "allowModules": ["camel-case", "capital-case", "snake-case"],
      "resolvePaths": [],
      "tryExtensions": []
    }]
  }
}
